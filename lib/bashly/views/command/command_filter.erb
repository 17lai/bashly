# :command.command_filter
<%- if commands.any? -%>
action=$1

case $action in
-* )
  ;;

<%- commands.each do |command| -%>
<%= command.aliases.join " | " %> )
  action="<%= command.name %>"
  shift
  <%= command.function_name %>_parse_requirements "$@"
  shift $#
  ;;    

<%- end -%>
* )
  <%= function_name %>_usage
  exit 1
  ;;

esac
<%- else -%>
action="<%= action_name %>"
<%- end -%>
