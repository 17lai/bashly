# :command.version_command
version_command() {
  echo "$version"
}

# :command.usage
docker_usage() {
  if [[ -n $long_usage ]]; then
    echo -e "docker - Docker example"
    echo 
  else
    echo -e "docker - Docker example"
    echo 
  fi
  echo -e "Usage:"
  echo -e "  docker [command] [options]"
  echo -e "  docker [command] --help | -h"
  echo -e "  docker --version"
  echo
  # :command.usage_commands
  echo -e "Commands:"
  echo "  container   Container commands"
  echo "  image       Image commands"
  echo

  if [[ -n $long_usage ]]; then
    echo -e "Options:"
    # :command.usage_fixed_flags
    echo "  --help, -h"
    echo -e "    Show this help"
    echo
    echo "  --version"
    echo -e "    Show version number"
    echo

  fi
}

